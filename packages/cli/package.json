{
  "name": "@fillet/cli",
  "version": "0.1.0",
  "description": "CLI for Fillet - Stripe Infrastructure as Code",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "fillet": "./bin/run"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": ["stripe", "iac", "cli"],
  "license": "MIT",
  "dependencies": {
    "@fillet/core": "workspace:*",
    "@fillet/constructs": "workspace:*",
    "@oclif/core": "^3.15.0",
    "@oclif/plugin-help": "^6.0.9",
    "stripe": "^14.9.0",
    "chalk": "^4.1.2",
    "diff": "^5.1.0",
    "esbuild-register": "^3.5.0"
  },
  "devDependencies": {
    "@oclif/test": "^3.1.3",
    "@types/diff": "^5.0.9",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  },
  "oclif": {
    "bin": "fillet",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  }
}
